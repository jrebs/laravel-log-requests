{
    "name": "jrebs/laravel-log-requests",
    "description": "Log requests and responses with minimal effort.",
    "type": "library",
    "require": {
        "php": "^7.2",
       "laravel/framework": ">=6.20.12"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Rebelo",
            "homepage": "https://rebelo.dev/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Jrebs\\LogRequests\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jrebs\\LogRequests\\Providers\\LogRequestsServiceProvider"
            ]
        }
    }
}
